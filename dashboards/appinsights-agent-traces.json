{
  "title": "AI Agent Traces - Application Insights",
  "description": "Agent execution traces from Application Insights. Includes operation durations, span analysis, and breakdowns by user, department, scenario, and service.",
  "tags": [
    "ai-agent",
    "application-insights",
    "traces",
    "observability",
    "maf"
  ],
  "timezone": "browser",
  "editable": true,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "grafana-azure-monitor-datasource",
        "current": {},
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "options": [],
        "refresh": 1,
        "regex": ""
      },
      {
        "name": "user_id",
        "type": "custom",
        "query": "All,user_001,user_002,user_003,user_004,user_005",
        "current": {
          "text": "All",
          "value": "All"
        },
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "options": []
      },
      {
        "name": "department",
        "type": "custom",
        "query": "All,Engineering,Marketing",
        "current": {
          "text": "All",
          "value": "All"
        },
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "options": []
      },
      {
        "name": "scenario_id",
        "type": "custom",
        "query": "All,local-maf,maf-with-fas,local-maf-multiagent",
        "current": {
          "text": "All",
          "value": "All"
        },
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "options": []
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "Overview",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "collapsed": false
    },
    {
      "type": "stat",
      "title": "Total Operations",
      "datasource": {
        "type": "grafana-azure-monitor-datasource",
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 1
      },
      "targets": [
        {
          "queryType": "Azure Log Analytics",
          "azureLogAnalytics": {
            "query": "let userFilter = '${user_id}';\nlet deptFilter = '${department}';\nlet scenarioFilter = '${scenario_id}';\nAppDependencies\n| where (userFilter == 'All' or tostring(Properties['user.id']) == userFilter)\n| where (deptFilter == 'All' or tostring(Properties['organization.department']) == deptFilter)\n| where (scenarioFilter == 'All' or tostring(Properties['scenario_id']) == scenarioFilter)\n| count",
            "resources": [
              "/subscriptions/673af34d-6b28-41dc-bc7b-f507418045e6/resourceGroups/rg-maf-observability/providers/Microsoft.OperationalInsights/workspaces/mafobs-dev-law"
            ]
          }
        }
      ],
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "blue",
                "value": null
              }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "title": "Average Duration (ms)",
      "datasource": {
        "type": "grafana-azure-monitor-datasource",
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 1
      },
      "targets": [
        {
          "queryType": "Azure Log Analytics",
          "azureLogAnalytics": {
            "query": "let userFilter = '${user_id}';\nlet deptFilter = '${department}';\nlet scenarioFilter = '${scenario_id}';\nAppDependencies\n| where (userFilter == 'All' or tostring(Properties['user.id']) == userFilter)\n| where (deptFilter == 'All' or tostring(Properties['organization.department']) == deptFilter)\n| where (scenarioFilter == 'All' or tostring(Properties['scenario_id']) == scenarioFilter)\n| summarize avg(DurationMs)",
            "resources": [
              "/subscriptions/673af34d-6b28-41dc-bc7b-f507418045e6/resourceGroups/rg-maf-observability/providers/Microsoft.OperationalInsights/workspaces/mafobs-dev-law"
            ]
          }
        }
      ],
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 1000
              },
              {
                "color": "red",
                "value": 5000
              }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "title": "Error Rate",
      "datasource": {
        "type": "grafana-azure-monitor-datasource",
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 1
      },
      "targets": [
        {
          "queryType": "Azure Log Analytics",
          "azureLogAnalytics": {
            "query": "let userFilter = '${user_id}';\nlet deptFilter = '${department}';\nlet scenarioFilter = '${scenario_id}';\nAppDependencies\n| where (userFilter == 'All' or tostring(Properties['user.id']) == userFilter)\n| where (deptFilter == 'All' or tostring(Properties['organization.department']) == deptFilter)\n| where (scenarioFilter == 'All' or tostring(Properties['scenario_id']) == scenarioFilter)\n| summarize total=count(), errors=countif(Success == false)\n| extend error_rate = round(100.0 * errors / total, 2)\n| project error_rate",
            "resources": [
              "/subscriptions/673af34d-6b28-41dc-bc7b-f507418045e6/resourceGroups/rg-maf-observability/providers/Microsoft.OperationalInsights/workspaces/mafobs-dev-law"
            ]
          }
        }
      ],
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 5
              },
              {
                "color": "red",
                "value": 20
              }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "title": "Unique Services",
      "datasource": {
        "type": "grafana-azure-monitor-datasource",
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 1
      },
      "targets": [
        {
          "queryType": "Azure Log Analytics",
          "azureLogAnalytics": {
            "query": "AppDependencies\n| union AppRequests\n| distinct AppRoleName\n| count",
            "resources": [
              "/subscriptions/673af34d-6b28-41dc-bc7b-f507418045e6/resourceGroups/rg-maf-observability/providers/Microsoft.OperationalInsights/workspaces/mafobs-dev-law"
            ]
          }
        }
      ],
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "purple",
                "value": null
              }
            ]
          }
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Operations Over Time by Service",
      "datasource": {
        "type": "grafana-azure-monitor-datasource",
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 5
      },
      "targets": [
        {
          "queryType": "Azure Log Analytics",
          "azureLogAnalytics": {
            "query": "let userFilter = '${user_id}';\nlet deptFilter = '${department}';\nlet scenarioFilter = '${scenario_id}';\nAppDependencies\n| where (userFilter == 'All' or tostring(Properties['user.id']) == userFilter)\n| where (deptFilter == 'All' or tostring(Properties['organization.department']) == deptFilter)\n| where (scenarioFilter == 'All' or tostring(Properties['scenario_id']) == scenarioFilter)\n| summarize count() by bin(TimeGenerated, 5m), AppRoleName\n| render timechart",
            "resources": [
              "/subscriptions/673af34d-6b28-41dc-bc7b-f507418045e6/resourceGroups/rg-maf-observability/providers/Microsoft.OperationalInsights/workspaces/mafobs-dev-law"
            ]
          }
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": [
            "sum"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2,
            "spanNulls": true
          }
        }
      }
    },
