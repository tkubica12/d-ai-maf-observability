{
  "title": "AI Agent Metrics - Prometheus",
  "description": "Custom agent metrics from Azure Monitor Prometheus. Includes agent call counts, token usage, and breakdowns by user, department, scenario, and VIP status.",
  "tags": [
    "ai-agent",
    "prometheus",
    "observability",
    "maf"
  ],
  "timezone": "browser",
  "editable": true,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus",
        "current": {},
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "options": [],
        "refresh": 1,
        "regex": ""
      },
      {
        "name": "user_id",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "query": "label_values(custom_agent_call_count_total, user_id)",
        "current": {},
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "options": [],
        "refresh": 2,
        "sort": 1,
        "allValue": ".*"
      },
      {
        "name": "department",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "query": "label_values(custom_agent_call_count_total, organization_department)",
        "current": {},
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "options": [],
        "refresh": 2,
        "sort": 1,
        "allValue": ".*"
      },
      {
        "name": "scenario_id",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "query": "label_values(custom_agent_call_count_total, scenario_id)",
        "current": {},
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "options": [],
        "refresh": 2,
        "sort": 1,
        "allValue": ".*"
      },
      {
        "name": "is_vip",
        "type": "custom",
        "query": "All : .*,true,false",
        "current": {
          "text": "All",
          "value": ".*"
        },
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "options": []
      },
      {
        "name": "token_type",
        "type": "custom",
        "query": "All : .*,input,output,total",
        "current": {
          "text": "All",
          "value": ".*"
        },
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "options": []
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "Overview",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "collapsed": false
    },
    {
      "type": "stat",
      "title": "Total Agent Calls",
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 1
      },
      "targets": [
        {
          "expr": "sum(custom_agent_call_count_total{user_id=~\"$user_id\", organization_department=~\"$department\", scenario_id=~\"$scenario_id\", user_is_vip=~\"$is_vip\"})",
          "legendFormat": "Total Calls"
        }
      ],
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "blue",
                "value": null
              },
              {
                "color": "green",
                "value": 10
              }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "title": "Total Token Usage",
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 1
      },
      "targets": [
        {
          "expr": "sum(custom_token_usage_tokens_total{user_id=~\"$user_id\", organization_department=~\"$department\", scenario_id=~\"$scenario_id\", user_is_vip=~\"$is_vip\", token_type=\"total\"})",
          "legendFormat": "Total Tokens"
        }
      ],
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "purple",
                "value": null
              },
              {
                "color": "orange",
                "value": 1000
              }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "title": "Unique Users",
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 1
      },
      "targets": [
        {
          "expr": "count(count by (user_id) (custom_agent_call_count_total{user_id=~\"$user_id\", organization_department=~\"$department\", scenario_id=~\"$scenario_id\"}))",
          "legendFormat": "Users"
        }
      ],
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "yellow",
                "value": null
              }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "title": "Active Scenarios",
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 1
      },
      "targets": [
        {
          "expr": "count(count by (scenario_id) (custom_agent_call_count_total{user_id=~\"$user_id\", organization_department=~\"$department\", scenario_id=~\"$scenario_id\"}))",
          "legendFormat": "Scenarios"
        }
      ],
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Agent Call Rate Over Time",
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 5
      },
      "targets": [
        {
          "expr": "sum(rate(custom_agent_call_count_total{user_id=~\"$user_id\", organization_department=~\"$department\", scenario_id=~\"$scenario_id\", user_is_vip=~\"$is_vip\"}[5m])) by (scenario_id)",
          "legendFormat": "{{scenario_id}}"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": [
            "sum",
            "mean"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2,
            "pointSize": 5,
            "spanNulls": true
          }
        }
      }
    },
